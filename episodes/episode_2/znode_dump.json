                  <filter class="solr.SnowballPorterFilterFactory" language="Dutch"/>
                </analyzer>
              </fieldType>
              
              <!-- Norwegian -->
              <dynamicField indexed="true" name="*_txt_no" stored="true" type="text_no"/>
              <fieldType class="solr.TextField" name="text_no" positionIncrementGap="100">
